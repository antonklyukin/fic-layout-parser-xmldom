{
  "env": {
    "es2021": true,
    "node": true
  },
  "plugins": ["prettier", "jest"],
  "extends": ["prettier", "eslint:recommended", "plugin:jest/all"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  }
}
